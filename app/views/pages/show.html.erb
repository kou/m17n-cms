<p>
  <b>Parent:</b>
  <%=h @page.parent_id %>
</p>

<p>
  <b>Name:</b>
  <%=h @page.name %>
</p>

<p>
  <b>Published:</b>
  <%=h @page.published %>
</p>


<%= link_to 'Edit', edit_page_path(@page) %> |
<%= link_to 'Back', pages_path %>

<div id="contents"></div>

<%= javascript_tag("
Ext.onReady(function() {
  var contents = new Ext.TabPanel({
    defaults: {autoScroll: true},
    items: #{content_tabs_data(@page).to_json}
  });
});") %>
